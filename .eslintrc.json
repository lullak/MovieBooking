{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
    "airbnb",
    "react-app",
    "react-app/jest",
    "plugin:prettier/recommended",
    "prettier/@typescript-eslint",
    "prettier/react"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "quotes": ["error", "double"]
    }
}